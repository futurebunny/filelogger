{
  "Logging": {
    // global filter settings
    "LogLevel": {
      "Default": "Information"
    },
    "File": {
      "IncludeScopes": true,
      "BasePath": "Logs",
      "LogLevel": {
        "Default": "Trace"
      },
      "TextBuilderType": "CustomFormat.SingleLineLogEntryTextBuilder, CustomFormat",
      "Files": [
        {
          "Path": "custom-sample-app-using-development-settings.log"
        }
      ]
    }
  }
}
